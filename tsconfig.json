    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@pages/*": ["pages/*"],
      "@store/*": ["store/*"],
      "@models/*": ["types/*"],
      "@theme/*": ["theme/*"],
